<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:EditConfig">
    <cq:dropTargets jcr:primaryType="nt:unstructured">
        <image1
            jcr:primaryType="cq:DropTargetConfig"
            sling:resourceType="foundation/components/image"
            accept="[image/.*]"
            groups="[media]"
            propertyName="./image1/fileReference">
            <parameters
                jcr:primaryType="nt:unstructured"
                sling:resourceType="foundation/components/imageimagetext"
                imageCrop=""
                imageMap=""
                imageRotate="">
                <image1
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="foundation/components/image"
                    imageCrop=""
                    imageMap=""
                    imageRotate=""/>
            </parameters>
        </image1>
        <image2
            jcr:primaryType="cq:DropTargetConfig"
            accept="[image/.*]"
            groups="[media]"
            propertyName="./image2/fileReference">
            <parameters
                jcr:primaryType="nt:unstructured"
                sling:resourceType="foundation/components/imageimagetext"
                imageCrop=""
                imageMap=""
                imageRotate="">
                <image2
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="foundation/components/image"
                    imageCrop=""
                    imageMap=""
                    imageRotate=""/>
            </parameters>
        </image2>
    </cq:dropTargets>
    <cq:inplaceEditing
        jcr:primaryType="cq:InplaceEditingConfig"
        configPath="../../dialog/items/tab1/items/text"
        active="{Boolean}true"
        editorType="hybrid">
        <cq:childEditors jcr:primaryType="nt:unstructured">
            <image1
                jcr:primaryType="cq:ChildEditorConfig"
                title="Image1"
                type="image"/>
            <image2
                jcr:primaryType="cq:ChildEditorConfig"
                title="Image2"
                type="image"/>
            <text
                jcr:primaryType="cq:ChildEditorConfig"
                title="Text"
                type="text"/>
        </cq:childEditors>
    </cq:inplaceEditing>
</jcr:root>
